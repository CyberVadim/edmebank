<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
        http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="000" author="Timoshenko_AS">
        <preConditions onFail="MARK_RAN">
            <not>
                <customPrecondition
                        className="liquibase.ext.preconditions.SchemaExistsPrecondition">
                    <param name="schemaName" value="client_data"/>
                </customPrecondition>
            </not>
        </preConditions>

        <sql>CREATE SCHEMA client_data;</sql>
    </changeSet>

</databaseChangeLog>